<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="mechcommander"
	OutputDirectory="$(SolutionDir)bin\$(PlatformName)_$(ConfigurationName)\"
	IntermediateDirectory="$(TEMP)\$(SolutionName)\$(ProjectName)\$(PlatformName)_$(ConfigurationName)\"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalOptions="-Wall -MP -sdl -utf-8 -guard:cf -Zo -std:c++latest -permissive- -Zc:__cplusplus -Zc:enumTypes -Zc:externConstexpr -Zc:inline -Zc:lambda -Zc:preprocessor -Zc:throwingNew"
		Optimization="3"
		EnableIntrinsicFunctions="true"
		FavorSizeOrSpeed="2"
		OmitFramePointers="true"
		AdditionalIncludeDirectories=".;$(IntDir);$(mech_inc);$(mclib_dir);$(xaudio2redist_dir)include;$(directxtk_dir)/inc;$(gameos_dir)/include;$(pch_dir);$(mc2res_dir)"
		PreprocessorDefinitions=""
		StringPooling="true"
		RuntimeLibrary="2"
		EnableFunctionLevelLinking="true"
		UsePrecompiledHeader="2"
		PrecompiledHeaderThrough="stdinc.h"
		CallingConvention="2"
	/>
	<Tool
		Name="VCLibrarianTool"
		IgnoreAllDefaultLibraries="true"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalOptions="-ignore:4199 -pdbcompress"
		AdditionalDependencies="kernel32.lib advapi32.lib ntdll.lib"
		Version="2.1"
		AdditionalLibraryDirectories="$(OutDir);$(IntDir)"
		DelayLoadDLLs="advapi32.dll;comdlg32.dll;ole32.dll;oleaut32.dll;shell32.dll;shlwapi.dll"
		GenerateDebugInformation="true"
		SubSystem="2"
		SetChecksum="true"
		SupportUnloadOfDelayLoadedDLL="true"
	/>
	<Tool
		Name="VCMIDLTool"
		GenerateStublessProxies="true"
		TypeLibraryName="$(IntDir)$(TargetName).tlb"
		OutputDirectory="$(IntDir)"
		HeaderFileName="$(IntDir)$(TargetName)_i.h"
		DLLDataFileName=""
		InterfaceIdentifierFileName="$(IntDir)$(TargetName)_i.c"
		ProxyFileName="$(IntDir)$(TargetName)_p.c"
	/>
	<Tool
		Name="VCResourceCompilerTool"
		PreprocessorDefinitions="PLATFORM=$(PlatformName);PROJECTNAME=L\&quot;$(ProjectName)\&quot;;TARGETFILENAME=L\&quot;$(TargetFileName)\&quot;;TARGETEXT=L\&quot;$(TargetExt)\&quot;"
		AdditionalIncludeDirectories=".;$(IntDir);$(mc2res_dir)"
	/>
  <UserMacro
		Name="xaudio2redist_dir"
		Value="../packages/Microsoft.XAudio2.Redist.1.2.2/build/native/"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="gameos_dir"
		Value="../gameos"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="directxtk_dir"
		Value="../directxtk/"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="mclib_dir"
		Value="../mclib"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="mechcmd2_dir"
		Value="../mechcmd2"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="mechgui_dir"
		Value="../mechcmd2/mechgui"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="mc2res_dir"
		Value="../mc2res"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="mech_inc"
		Value="../include"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="pch_dir"
		Value="../pch"
		PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>
